<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <corners android:radius="7.0dip" />
            <solid android:color="#ffff6502" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="false" android:color="@color/serviceNumber">
        <shape>
            <corners android:radius="7.0dip" />
            <solid android:color="@color/serviceNumber" />
        </shape>
    </item>
    <item android:state_enabled="false" android:color="@color/page_bg">
        <shape>
            <corners android:radius="7.0dip" />
            <solid android:color="#ffd9d9d9" />
        </shape>
    </item>
</selector>